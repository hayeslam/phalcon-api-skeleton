{
  "name": "hayes/phalcon-api-skeleton",
  "description": "A Phalcon api project skeleton.",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "dev-master",
  "require": {
    "php": "^7.4",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-phalcon": "~4.1.0",
    "ext-mbstring": "*",
    "vlucas/phpdotenv": "^5.4",
    "filp/whoops": "^2.14",
    "fig/http-message-util": "^1.1",
    "monolog/monolog": "^2.3"
  },
  "require-dev": {
    "phalcon/dd": "^1.1",
    "phalcon/ide-stubs": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "./bootstrap/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
