{
  "name": "hayes/phalcon-api-skeleton",
  "description": "A Phalcon api project skeleton.",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-phalcon": "~4.1.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phalcon/dd": "^1.1",
    "phalcon/ide-stubs": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
